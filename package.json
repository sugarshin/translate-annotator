{
  "name": "translate-annotator",
  "description": "translate-annotator",
  "scripts": {
    "start": "babel-node ./bin/www --presets es2015,stage-0",
    "lint": "eslint .",
    "nodemon": "nodemon --ignore client/ --ignore public/js/ -e js,jade,styl --exec npm start",
    "client:build": "browserify client/index.js -t [ babelify --presets [ es2015 stage-0 react ] ] --extension='.jsx' -p licensify | uglifyjs -m -c warnings=false --comments '/This header is generated by licensify/' -o public/js/index.js",
    "client:watch": "watchify client/index.js -v -t [ babelify --presets [ es2015 stage-0 react ] ] --extension='.jsx' -o public/js/index.js",
    "dev": "npm run client:watch & npm run nodemon",
    "postinstall": "npm run client:build"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/sugarshin/translate-annotator"
  },
  "author": "sugarshin",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.1.2",
    "babel-eslint": "^4.1.5",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "babel-preset-stage-0": "^6.1.2",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "eslint": "^1.9.0",
    "eslint-config-sugarshin": "0.0.13",
    "eslint-plugin-babel": "^2.1.1",
    "eslint-plugin-react": "^3.8.0",
    "licensify": "^2.0.1",
    "nodemon": "^1.8.1",
    "uglify-js": "^2.5.0",
    "watchify": "^3.6.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.0.16",
    "body-parser": "^1.14.1",
    "classnames": "^2.2.0",
    "cookie-parser": "^1.4.0",
    "debug": "^2.2.0",
    "express": "^4.13.3",
    "jade": "^1.11.0",
    "jsonp-p": "^1.0.1",
    "lodash": "^3.10.1",
    "morgan": "^1.6.1",
    "qs": "^5.2.0",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4",
    "redux-logger": "^2.0.4",
    "redux-thunk": "^1.0.0",
    "request-promise": "^1.0.2",
    "serve-favicon": "^2.3.0",
    "stylus": "^0.52.4",
    "whatwg-fetch": "^0.10.1"
  }
}

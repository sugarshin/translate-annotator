{
  "name": "translate-annotator",
  "description": "translate-annotator",
  "scripts": {
    "start": "babel-node ./bin/www --presets es2015,stage-0",
    "lint": "eslint .",
    "frontend:build": "browserify frontend/main.js -t [ babelify --presets [ es2015 stage-0 ] ] -p licensify | uglifyjs -m -c warnings=false --comments '/This header is generated by licensify/' -o public/js/main.js",
    "frontend:watch": "watchify frontend/main.js -v -t [ babelify --presets [ es2015 stage-0 ] ] -o public/js/main.js",
    "dev": "npm run frontend:watch & npm start",
    "postinstall": "npm run frontend:build"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/sugarshin/translate-annotator"
  },
  "author": "sugarshin",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.1.2",
    "babel-eslint": "^4.1.4",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-stage-0": "^6.1.2",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "eslint": "^1.8.0",
    "eslint-config-sugarshin": "0.0.11",
    "eslint-plugin-babel": "^2.1.1",
    "eslint-plugin-react": "^3.7.1",
    "licensify": "^2.0.1",
    "uglify-js": "^2.5.0",
    "watchify": "^3.6.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.0.16",
    "body-parser": "^1.14.1",
    "cookie-parser": "^1.4.0",
    "debug": "^2.2.0",
    "express": "^4.13.3",
    "jade": "^1.11.0",
    "jsonp-p": "^1.0.1",
    "lodash": "^3.10.1",
    "morgan": "^1.6.1",
    "qs": "^5.2.0",
    "request-promise": "^1.0.2",
    "serve-favicon": "^2.3.0",
    "stylus": "^0.52.4",
    "whatwg-fetch": "^0.10.1"
  }
}

{
  "name": "translate-annotator",
  "description": "translate-annotator",
  "scripts": {
    "start": "babel-node ./bin/www",
    "frontend:build:js": "browserify frontend/main.js -t babelify -p licensify | uglifyjs -m -c warnings=false --comments '/This header is generated by licensify/' -o public/js/main.js",
    "dev": "npm ru frontend:watch:js & npm start",
    "frontend:watch:js": "watchify frontend/main.js -v -t babelify -o public/js/main.js"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/sugarshin/translate-annotator"
  },
  "author": "sugarshin",
  "license": "MIT",
  "devDependencies": {
    "babelify": "^6.3.0",
    "browserify": "^11.2.0",
    "licensify": "^2.0.1",
    "uglify-js": "^2.4.24",
    "watchify": "^3.4.0"
  },
  "dependencies": {
    "babel": "^5.8.23",
    "bluebird": "^2.10.2",
    "body-parser": "~1.14.1",
    "cookie-parser": "~1.4.0",
    "debug": "~2.2.0",
    "express": "~4.13.3",
    "jade": "~1.11.0",
    "jsonp-p": "^1.0.0",
    "lodash": "^3.10.1",
    "morgan": "~1.6.1",
    "qs": "^5.1.0",
    "request-promise": "^0.4.3",
    "serve-favicon": "~2.3.0",
    "whatwg-fetch": "^0.9.0"
  }
}
